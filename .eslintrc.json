{
	"extends": "airbnb",
	"plugins": [
		"prettier"
	],
	"rules": {
		"no-underscore-dangle": 0,
		"no-console": 0,
		"react/no-array-index-key": 0,
		"no-tabs": 0,
		"react/forbid-prop-types": 0,
		"no-mixed-spaces-and-tabs": 0,
		"react/jsx-indent": 0,
		"indent": 0,
		"comma-dangle": 0,
		"react/jsx-indent-props": 0,
		"arrow-parens": 0,
		"react/jsx-one-expression-per-line": 0
	}
}